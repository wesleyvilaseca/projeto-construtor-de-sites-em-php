{
    "name": "naucon/htmlbuilder",
    "description": "A package for php to generate html markup with object representation but without the overhead of domXML or SimpelXML.",
    "keywords": ["html", "markup", "builder", "generate", "render", "create"],
    "homepage": "https://github.com/naucon/HtmlBuilder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Sanzenbacher"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "naucon/utility": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {"Naucon\\HtmlBuilder\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Naucon\\HtmlBuilder\\Tests\\": "tests"}
    }
}
